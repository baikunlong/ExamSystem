<ScrollPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.172-ea"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.ExamController">
    <content>
        <AnchorPane prefHeight="900.0" prefWidth="880.0">
            <children>
                <Label layoutX="14.0" layoutY="14.0" text="课程："/>
                <Label layoutX="153.0" layoutY="14.0" text="单选题数量："/>
                <Label layoutX="285.0" layoutY="14.0" text="多选题数量："/>
                <Label layoutX="415.0" layoutY="14.0" text="判断题数量："/>
                <Label layoutX="544.0" layoutY="14.0" text="填空题数量："/>
                <Button fx:id="btnStart" layoutX="698.0" layoutY="9.0" mnemonicParsing="false" text="开始考试"/>
                <ChoiceBox fx:id="cbCourse" layoutX="48.0" layoutY="9.0" prefHeight="30.0" prefWidth="92.0"/>
                <ChoiceBox fx:id="cbSingle" layoutX="238.0" layoutY="9.0" prefHeight="30.0" prefWidth="47.0"/>
                <ChoiceBox fx:id="cbMulti" layoutX="368.0" layoutY="9.0" prefHeight="30.0" prefWidth="47.0"/>
                <ChoiceBox fx:id="cbJudge" layoutX="497.0" layoutY="9.0" prefHeight="30.0" prefWidth="47.0"/>
                <ChoiceBox fx:id="cbCompletion" layoutX="627.0" layoutY="9.0" prefHeight="30.0" prefWidth="47.0"/>
                <VBox fx:id="vbox" layoutX="14" layoutY="70.0" prefHeight="941.0" prefWidth="860.0"/>
            </children>
        </AnchorPane>
    </content>
</ScrollPane>
